\t\t\t\t\t<i class=\"fa fa-edit\" @click=\"editUserAddress(item.daId)\"></i>\r\n\t\t\t\t\t<i class=\"fa fa-remove\" @click=\"removeUserAddress(item.daId)\"></i>\r\n\t\t\t\t</div>\r\n\t\t\t</li>\r\n\t\t</ul>\r\n\t\t<!-- 新增地址部分 -->\r\n\t\t<div class=\"addbtn\" @click=\"toAddUserAddress\">\r\n\t\t\t<i class=\"fa fa-plus-circle\"></i>\r\n\t\t\t<p>新增收货地址</p>\r\n\t\t</div>\r\n\t\t<!-- 底部菜单部分 -->\r\n\t\t<Footer></Footer>\r\n\t</div>\r\n</template>\r\n<script>\r\n\timport {\r\n\t\tref,\r\n\t\tonMounted,\r\n\t\tcomputed, \r\n\t\treactive \r\n\t} from 'vue';\r\n\timport {\r\n\t\tuseRoute,\r\n\t\tuseRouter\r\n\t} from 'vue-router';\r\n\timport axios from 'axios';\r\n\timport qs from 'qs';\r\n\timport Footer from '../components/Footer.vue';\r\n\timport { setLocalStorage,getLocalStorage,removeLocalStorage } from '../localStorageUtils.js';\r\n\r\n\texport default {\r\n\t\tname: 'UserAddress',\r\n\t\tcomponents: {\r\n\t\t\tFooter\r\n\t\t},\r\n\t\tsetup() {\r\n\t\t\tconst user = ref(null);\r\n\t\t\tconst businessId = ref(null);\r\n\t\t\tconst deliveryAddressArr = ref([]);\r\n\t\t\tconst route = useRoute();\r\n\t\t\tconst router = useRouter();\r\n\t\t\tconst daId = ref(null);\r\n\r\n\t\t\t//businessId.value = route.query.businessId;\r\n\t\t\t// 获取会话存储中的用户数据\r\n\t\t\t//user.value = JSON.parse(sessionStorage.getItem('user'));\r\n\r\n\t\t\tonMounted(async () => {\r\n\t\t\t\tuser.value = JSON.parse(sessionStorage.getItem('user'));\r\n\t\t\t\tbusinessId.value = route.query.businessId;\r\n\t\t\t\tlistDeliveryAddressByUserId();\r\n\t\t\t});\r\n\r\n\t\t\tconst sexFilter = (value) => {\r\n\t\t\t\treturn value == 1 ? '先生' : '女士';\r\n\t\t\t};\r\n\r\n\t\t\tconst listDeliveryAddressByUserId = () => {\r\n\t\t\t\taxios\r\n\t\t\t\t\t.post('DeliveryAddressController/listDeliveryAddressByUserId', qs.stringify({\r\n\t\t\t\t\t\tuserId: user.value.userId,\r\n\t\t\t\t\t}))\r\n\t\t\t\t\t.then(response => {\r\n\t\t\t\t\t\tdeliveryAddressArr.value = response.data;\r\n\t\t\t\t\t})\r\n\t\t\t\t\t.catch(error =