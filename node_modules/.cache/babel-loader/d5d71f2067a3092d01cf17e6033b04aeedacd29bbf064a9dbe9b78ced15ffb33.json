{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted } from 'vue';\nimport Footer from '../components/Footer.vue';\nimport axios from 'axios';\nimport { useRoute } from 'vue-router';\nimport router from '../router/index.js';\nimport qs from 'qs';\nexport default {\n  name: 'BusinessList',\n  components: {\n    Footer\n  },\n  setup() {\n    const orderTypeId = ref(null);\n    const businessArr = ref([]);\n    const route = useRoute();\n    const user = ref(null);\n    const fetchData = async () => {\n      //console.log(orderTypeId.value);\n      try {\n        //console.log(orderTypeId.value);\n        const response = await axios.post('BusinessController/listBusinessByOrderTypeId', null, {\n          params: {\n            orderTypeId: orderTypeId.value\n          }\n        });\n        /*const response = await axios.post('BusinessController/listBusinessByOrderTypeId', {\r\n                          orderTypeId: orderTypeId.value*/\n        businessArr.value = response.data;\n        //判断是否登录\n        if (user.value != null) {\n          listCart();\n        }\n      } catch (error) {\n        console.error(error);\n        console.log(error.message);\n        console.log(error.stack);\n      }\n    };\n    const toBusinessInfo = businessId => {\n      router.push({\n        path: '/businessInfo',\n        query: {\n          businessId: businessId\n        }\n      });\n    };\n    const listCart = () => {\n      axios.post('CartController/listCart', qs.stringify({\n        userId: user.value.userId\n      })).then(response => {\n        let cartArr = response.data;\n        //遍历所有食品列表，设置购物车中食品数量\n        for (let foodItem of foodArr.value) {\n          foodItem.quantity = 0;\n          for (let cartItem of cartArr) {\n            if (cartItem.businessId == foodItem.businessId) {\n              businessItem.quantity += cartItem.quantity;\n            }\n          }\n        }\n        businessArr.value.sort(); //确保vue监听到了它的变化\n      }).catch(error => {\n        console.error(error);\n      });\n    }; //////////////////////////////////////////////////////////////////\n\n    onMounted(() => {\n      const routeQuery = route.query;\n      orderTypeId.value = parseInt(routeQuery.orderTypeId);\n      user.value = JSON.parse(localStorage.getItem('user'));\n      fetchData();\n    });\n    return {\n      orderTypeId,\n      businessArr,\n      toBusinessInfo\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","Footer","axios","useRoute","router","qs","name","components","setup","orderTypeId","businessArr","route","user","fetchData","response","post","params","value","data","listCart","error","console","log","message","stack","toBusinessInfo","businessId","push","path","query","stringify","userId","then","cartArr","foodItem","foodArr","quantity","cartItem","businessItem","sort","catch","routeQuery","parseInt","JSON","parse","localStorage","getItem"],"sources":["F:\\myvue1\\elmclient\\elmclient\\src\\views\\BusinessList.vue"],"sourcesContent":["<template>\r\n\t<div class=\"wrapper\">\r\n\t\t<!--header部分-->\r\n\t\t<header>\r\n\t\t\t<p>商家列表</p>\r\n\t\t</header>\r\n\t\t<!--商家列表部分-->\r\n\t\t<ul class=\"business\">\r\n\t\t\t<li v-for=\"item in businessArr\" @click=\"toBusinessInfo(item.businessId)\">\r\n\t\t\t\t<div class=\"business-img\">\r\n\t\t\t\t\t<img :src=\"item.businessImg\"> <!--绑定属性-->\r\n\t\t\t\t\t<div class=\"business-img-quantity\" v-show=\"item.quantity>0\">{{item.quantity}}</div> <!--大于0时显示数量-->\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"business-info\">\r\n\t\t\t\t\t<h3>{{item.businessName}}</h3>\r\n\t\t\t\t\t<p>&#165; {{item.starPrice}}起送 | &#165;{{item.deliveryPrice}}配送</p>\r\n\t\t\t\t\t<p>{{item.businessExplain}}</p>\r\n\t\t\t\t</div>\r\n\t\t\t</li>\r\n\t\t</ul>\r\n\r\n\t\t<!--底部菜单部分-->\r\n\t\t<Footer></Footer>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\timport {ref,onMounted} from 'vue';\r\n\timport Footer from '../components/Footer.vue';\r\n\timport axios from 'axios';\r\n\timport {useRoute} from 'vue-router';\r\n\timport router from '../router/index.js';\r\n\timport qs from 'qs';\r\n\r\n\texport default {\r\n\t\tname: 'BusinessList',\r\n\t\tcomponents: {\r\n\t\t\tFooter,\r\n\t\t},\r\n\t\tsetup() {\r\n\t\t\tconst orderTypeId = ref(null);\r\n\t\t\tconst businessArr = ref([]);\r\n\t\t\tconst route = useRoute();\r\n\t\t\tconst user = ref(null);\r\n\r\n\t\t\tconst fetchData = async () => {\r\n\t\t\t\t//console.log(orderTypeId.value);\r\n\t\t\t\ttry {\r\n\t\t\t\t\t//console.log(orderTypeId.value);\r\n\t\t\t\t\tconst response = await axios.post('BusinessController/listBusinessByOrderTypeId', null, {\r\n\t\t\t\t\t\tparams: {\r\n\t\t\t\t\t\t\torderTypeId: orderTypeId.value\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t});\r\n\t\t\t\t\t/*const response = await axios.post('BusinessController/listBusinessByOrderTypeId', {\r\n\t\t                     orderTypeId: orderTypeId.value*/\r\n\t\t\t\t\tbusinessArr.value = response.data;\r\n\t\t\t\t\t//判断是否登录\r\n\t\t\t\t\tif(user.value != null){\r\n\t\t\t\t\t\tlistCart();\r\n\t\t\t\t\t}\r\n\r\n\t\t\t\t} catch (error) {\r\n\t\t\t\t\tconsole.error(error);\r\n\t\t\t\t\tconsole.log(error.message);\r\n\t\t\t\t\tconsole.log(error.stack);\r\n\t\t\t\t}\r\n\t\t\t};\r\n\t\t\t\r\n\t\t\tconst toBusinessInfo = (businessId) => {\r\n\t\t\t  router.push({ path: '/businessInfo', query: { businessId: businessId } });\r\n\t\t\t};\r\n\t\t\t\r\n\t\t\tconst listCart = () => {\r\n\t\t\t\taxios\r\n\t\t\t\t\t.post('CartController/listCart', qs.stringify({\r\n\t\t\t\t\t\tuserId: user.value.userId,\r\n\t\t\t\t\t}))\r\n\t\t\t\t\t.then(response => {\r\n\t\t\t\t\t\tlet cartArr = response.data;\r\n\t\t\t\t\t\t//遍历所有食品列表，设置购物车中食品数量\r\n\t\t\t\t\t\tfor (let foodItem of foodArr.value) {\r\n\t\t\t\t\t\t\tfoodItem.quantity = 0;\r\n\t\t\t\t\t\t\tfor (let cartItem of cartArr) {\r\n\t\t\t\t\t\t\t\tif (cartItem.businessId == foodItem.businessId) {\r\n\t\t\t\t\t\t\t\t\tbusinessItem.quantity += cartItem.quantity;\r\n\t\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tbusinessArr.value.sort();//确保vue监听到了它的变化\r\n\t\t\t\t\t})\r\n\t\t\t\t\t.catch(error => {\r\n\t\t\t\t\t\tconsole.error(error);\r\n\t\t\t\t\t});\r\n\t\t\t}; //////////////////////////////////////////////////////////////////\r\n\r\n            onMounted(() => {\r\n            \tconst routeQuery = route.query;\r\n            \torderTypeId.value = parseInt(routeQuery.orderTypeId);\r\n            \tuser.value = JSON.parse(localStorage.getItem('user'));\r\n            \tfetchData();\r\n            });\r\n\r\n\t\t\treturn {\r\n\t\t\t\torderTypeId,\r\n\t\t\t\tbusinessArr,\r\n\t\t\t\ttoBusinessInfo\r\n\t\t\t};\r\n\t\t},\r\n\r\n\r\n\t};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n\t/***总容器***/\r\n\t.wrapper {\r\n\t\twidth: 100%;\r\n\t\theight: 100%;\r\n\t}\r\n\r\n\t/****header部分***/\r\n\t.wrapper header {\r\n\t\twidth: 100%;\r\n\t\theight: 12vw;\r\n\t\tbackground-color: #0097FF;\r\n\t\tcolor: #fff;\r\n\t\tfont-size: 4.8vw;\r\n\r\n\t\tposition: fixed;\r\n\t\tleft: 0;\r\n\t\ttop: 0;\r\n\t\tz-index: 1000;\r\n\t\t/*层次提升*/\r\n\r\n\t\tdisplay: flex;\r\n\t\tjustify-content: center;\r\n\t\talign-items: center;\r\n\t}\r\n\r\n\t/***商家列表部分***/\r\n\t.wrapper .business {\r\n\t\twidth: 100%;\r\n\t\tmargin-top: 12vw;\r\n\t\tpadding-bottom: 14vw;\r\n\t\t/*margin-bottom: 14vw;*/\r\n\t}\r\n\r\n\t.wrapper .business li {\r\n\t\twidth: 100%;\r\n\t\tbox-sizing: border-box;\r\n\t\tpadding: 2.5vw;\r\n\t\tborder-bottom: solid 1px #DDD;\r\n\t\tuser-select: none;\r\n\t\tcursor: pointer;\r\n\r\n\t\tdisplay: flex;\r\n\t\talign-items: center;\r\n\t}\r\n\r\n\t.wrapper .business li .business-img {\r\n\t\tposition: relative;\r\n\t}\r\n\r\n\t.wrapper .business li .business-img img {\r\n\t\twidth: 20vw;\r\n\t\theight: 20vw;\r\n\t}\r\n\r\n\t.wrapper .business li .business-img .business-img-quantity {\r\n\t\twidth: 5vw;\r\n\t\theight: 5vw;\r\n\t\tbackground-color: red;\r\n\t\tcolor: #fff;\r\n\t\tfont-size: 3.6vw;\r\n\t\tborder-radius: 2.5vw;\r\n\r\n\t\tdisplay: flex;\r\n\t\tjustify-content: center;\r\n\t\talign-items: center;\r\n\r\n\t\tposition: absolute;\r\n\t\tright: -1.5vw;\r\n\t\ttop: -1.5vw;\r\n\t}\r\n\r\n\t.wrapper .business li .business-info {\r\n\t\tmargin-left: 3vw;\r\n\t}\r\n\r\n\t.wrapper .business li .business-info h3 {\r\n\t\tfont-size: 3.8vw;\r\n\t\tcolor: #555;\r\n\t}\r\n\r\n\t.wrapper .business li .business-info p {\r\n\t\tfont-size: 3vw;\r\n\t\tcolor: #888;\r\n\t\tmargin-top: 2vw;\r\n\t}\r\n</style>"],"mappings":";AA2BC,SAAQA,GAAG,EAACC,SAAS,QAAO,KAAK;AACjC,OAAOC,MAAK,MAAO,0BAA0B;AAC7C,OAAOC,KAAI,MAAO,OAAO;AACzB,SAAQC,QAAQ,QAAO,YAAY;AACnC,OAAOC,MAAK,MAAO,oBAAoB;AACvC,OAAOC,EAAC,MAAO,IAAI;AAEnB,eAAe;EACdC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IACXN;EACD,CAAC;EACDO,KAAKA,CAAA,EAAG;IACP,MAAMC,WAAU,GAAIV,GAAG,CAAC,IAAI,CAAC;IAC7B,MAAMW,WAAU,GAAIX,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMY,KAAI,GAAIR,QAAQ,CAAC,CAAC;IACxB,MAAMS,IAAG,GAAIb,GAAG,CAAC,IAAI,CAAC;IAEtB,MAAMc,SAAQ,GAAI,MAAAA,CAAA,KAAY;MAC7B;MACA,IAAI;QACH;QACA,MAAMC,QAAO,GAAI,MAAMZ,KAAK,CAACa,IAAI,CAAC,8CAA8C,EAAE,IAAI,EAAE;UACvFC,MAAM,EAAE;YACPP,WAAW,EAAEA,WAAW,CAACQ;UAC1B;QACD,CAAC,CAAC;QACF;;QAEAP,WAAW,CAACO,KAAI,GAAIH,QAAQ,CAACI,IAAI;QACjC;QACA,IAAGN,IAAI,CAACK,KAAI,IAAK,IAAI,EAAC;UACrBE,QAAQ,CAAC,CAAC;QACX;MAED,EAAE,OAAOC,KAAK,EAAE;QACfC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;QACpBC,OAAO,CAACC,GAAG,CAACF,KAAK,CAACG,OAAO,CAAC;QAC1BF,OAAO,CAACC,GAAG,CAACF,KAAK,CAACI,KAAK,CAAC;MACzB;IACD,CAAC;IAED,MAAMC,cAAa,GAAKC,UAAU,IAAK;MACrCtB,MAAM,CAACuB,IAAI,CAAC;QAAEC,IAAI,EAAE,eAAe;QAAEC,KAAK,EAAE;UAAEH,UAAU,EAAEA;QAAW;MAAE,CAAC,CAAC;IAC3E,CAAC;IAED,MAAMP,QAAO,GAAIA,CAAA,KAAM;MACtBjB,KAAI,CACFa,IAAI,CAAC,yBAAyB,EAAEV,EAAE,CAACyB,SAAS,CAAC;QAC7CC,MAAM,EAAEnB,IAAI,CAACK,KAAK,CAACc;MACpB,CAAC,CAAC,EACDC,IAAI,CAAClB,QAAO,IAAK;QACjB,IAAImB,OAAM,GAAInB,QAAQ,CAACI,IAAI;QAC3B;QACA,KAAK,IAAIgB,QAAO,IAAKC,OAAO,CAAClB,KAAK,EAAE;UACnCiB,QAAQ,CAACE,QAAO,GAAI,CAAC;UACrB,KAAK,IAAIC,QAAO,IAAKJ,OAAO,EAAE;YAC7B,IAAII,QAAQ,CAACX,UAAS,IAAKQ,QAAQ,CAACR,UAAU,EAAE;cAC/CY,YAAY,CAACF,QAAO,IAAKC,QAAQ,CAACD,QAAQ;YAC3C;UACD;QAC