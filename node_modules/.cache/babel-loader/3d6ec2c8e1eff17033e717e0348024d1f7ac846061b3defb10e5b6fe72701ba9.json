{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted, watch, computed } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport axios from 'axios';\nimport qs from 'qs';\nexport default {\n  name: 'BusinessInfo',\n  setup() {\n    const route = useRoute();\n    const router = useRouter();\n    const businessId = ref(null);\n    //const foodId = ref(null);\n    const business = ref({});\n    const foodArr = ref([]);\n    const user = ref(null);\n    const cartArr = ref([]);\n    const deliveryPrice = ref(0);\n    const fetchBusinessInfo = () => {\n      axios.post('BusinessController/getBusinessById', qs.stringify({\n        businessId: businessId.value\n      })).then(response => {\n        business.value = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    };\n    const fetchFoodInfo = () => {\n      axios.post('FoodController/listFoodByBusinessId', qs.stringify({\n        businessId: businessId.value\n      })).then(response => {\n        foodArr.value = response.data;\n        for (let i = 0; i < foodArr.length; i++) {\n          foodArr.value[i].quantity = 0;\n        }\n        //如果已登录，那么需要去查询购物车中是否已经选购了某个食品\n        if (user != null) {\n          listCart();\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    };\n    const getSessionStorage = () => {\n      const storedUser = sessionStorage.getItem('user');\n      user.value = storedUser ? JSON.parse(storedUser) : null;\n    };\n    const add = index => {\n      if (user.value === null) {\n        router.push({\n          path: '/login'\n        });\n        return;\n      }\n      if (foodArr.value[index].quantity === 0) {\n        saveCart(index);\n      } else {\n        updateCart(index, 1);\n      }\n    };\n    const minus = index => {\n      if (user.value === null) {\n        router.push({\n          path: '/login'\n        });\n        return;\n      }\n      if (foodArr.value[index].quantity > 1) {\n        updateCart(index, -1);\n      } else {\n        removeCart(index);\n      }\n    };\n    const listCart = () => {\n      axios.post('CartController/listCart', qs.stringify({\n        businessId: businessId.value,\n        userId: user.value.userId\n        //foodId: foodArr.value[index].foodId\n      })).then(response => {\n        let cartArr = response.data;\n        //遍历所有食品列表，设置购物车中食品数量\n        for (let foodItem of foodArr.value) {\n          foodItem.quantity = 0;\n          for (let cartItem of cartArr) {\n            if (cartItem.foodId == foodItem.foodId) {\n              foodItem.quantity = cartItem.quantity;\n            }\n          }\n        }\n        foodArr.value.sort(); //确保vue监听到了它的变化\n      }).catch(error => {\n        console.error(error);\n      });\n    }; //////////////////////////////////////////////////////////////////\n\n    const saveCart = index => {\n      axios.post('CartController/saveCart', qs.stringify({\n        businessId: businessId.value,\n        userId: user.value.userId,\n        foodId: foodArr.value[index].foodId\n      })).then(response => {\n        if (response.data === 1) {\n          foodArr.value[index].quantity = 1;\n          const arr = [...foodArr.value]; // 将响应式变量转换为数组\n          arr.sort(); // 对数组进行排序\n          foodArr.value = arr; // 将排序后的数组赋值回响应式变量\n        } else {\n          alert('向购物车中添加食品失败');\n        }\n        console.log('saveadd');\n        console.log(response.data);\n        console.log('saveadd');\n      }).catch(error => {\n        console.error(error);\n      });\n    };\n    const updateCart = (index, num) => {\n      axios.post('CartController/updateCart', qs.stringify({\n        businessId: businessId.value,\n        userId: user.value.userId,\n        foodId: foodArr.value[index].foodId,\n        quantity: foodArr.value[index].quantity + num\n      })).then(response => {\n        if (response.data === 1) {\n          foodArr.value[index].quantity += num;\n          const arr = [...foodArr.value]; // 将响应式变量转换为数组\n          arr.sort(); // 对数组进行排序\n          foodArr.value = arr; // 将排序后的数组赋值回响应式变量\n        } else {\n          alert('向购物车中更新食品失败!');\n          console.log(response.data);\n        }\n        console.log('update');\n        console.log(response.data);\n        console.log('update');\n      }).catch(error => {\n        console.error(error);\n      });\n    };\n    const removeCart = index => {\n      axios.post('CartController/removeCart', qs.stringify({\n        businessId: businessId.value,\n        userId: user.value.userId,\n        foodId: foodArr.value[index].foodId\n      })).then(response => {\n        if (response.data === 1) {\n          foodArr.value[index].quantity = 0;\n          const arr = [...foodArr.value]; // 将响应式变量转换为数组\n          arr.sort(); // 对数组进行排序\n          foodArr.value = arr; // 将排序后的数组赋值回响应式变量\n        } else {\n          alert('向购物车中删除食品失败!');\n        }\n        console.log('remove');\n        console.log(response.data);\n        console.log('remove');\n      }).catch(error => {\n        console.error(error);\n      });\n    };\n    const totalPrice = computed(() => {\n      let total = 0;\n      for (let item of foodArr.value) {\n        total += item.foodPrice * item.quantity;\n      }\n      return total;\n    });\n    const totalQuantity = computed(() => {\n      let quantity = 0;\n      for (let item of foodArr.value) {\n        quantity += item.quantity;\n      }\n      return quantity;\n    });\n    const totalSettle = computed(() => {\n      return totalPrice.value + deliveryPrice.value;\n    });\n    const toOrder = () => {\n      router.push({\n        path: '/orders',\n        query: {\n          businessId: business.businessId\n        }\n      });\n    };\n    watch(() => route.query.businessId, newBusinessId => {\n      businessId.value = newBusinessId;\n      fetchBusinessInfo();\n      fetchFoodInfo();\n      getSessionStorage();\n    }, {\n      immediate: true\n    }\n    // 立即执行回调函数，以便初始化 businessId\n    );\n\n    onMounted(() => {\n      if (user.value !== null) {\n        listCart(0);\n      }\n    });\n    return {\n      businessId,\n      business,\n      foodArr,\n      user,\n      add,\n      minus,\n      totalQuantity,\n      totalPrice,\n      totalSettle,\n      toOrder\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","watch","computed","useRoute","useRouter","axios","qs","name","setup","route","router","businessId","business","foodArr","user","cartArr","deliveryPrice","fetchBusinessInfo","post","stringify","value","then","response","data","catch","error","console","fetchFoodInfo","i","length","quantity","listCart","getSessionStorage","storedUser","sessionStorage","getItem","JSON","parse","add","index","push","path","saveCart","updateCart","minus","removeCart","userId","foodItem","cartItem","foodId","sort","arr","alert","log","num","totalPrice","total","item","foodPrice","totalQuantity","totalSettle","toOrder","query","newBusinessId","immediate"],"sources":["F:\\myvue1\\elmclient\\elmclient\\src\\views\\BusinessInfo.vue"],"sourcesContent":["<template>\r\n\t<div class=\"wrapper\">\r\n\t\t<!-- header部分 -->\r\n\t\t<header>\r\n\t\t\t<p>商家信息</p>\r\n\t\t</header>\r\n\r\n\t\t<!-- 商家logo部分 -->\r\n\t\t<div class=\"business-logo\">\r\n\t\t\t<img :src=\"business.businessImg\">\r\n\t\t</div>\r\n\r\n\t\t<!-- 商家信息部分 -->\r\n\t\t<div class=\"business-info\">\r\n\t\t\t<h1>{{business.businessName}}</h1>\r\n\t\t\t<p>&#165;{{business.starPrice}}起送 &#165;{{business.deliveryPrice}}配送</p>\r\n\t\t\t<p>{{business.businessExplain}}</p>\r\n\t\t</div>\r\n\r\n\t\t<!-- 食品列表部分\"\" -->\r\n\t\t<ul class=\"food\">\r\n\t\t\t<li v-for=\"(item,index) in foodArr\">\r\n\t\t\t\t<div class=\"food-left\">\r\n\t\t\t\t\t<img :src=\"item.foodImg\">\r\n\t\t\t\t\t<div class=\"food-left-info\">\r\n\t\t\t\t\t\t<h3>{{item.foodName}}</h3>\r\n\t\t\t\t\t\t<p>{{item.foodExplain}}</p>\r\n\t\t\t\t\t\t<p>&#165;{{item.foodPrice}}</p>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"food-right\">\r\n\t\t\t\t\t<div>\r\n\t\t\t\t\t\t<i class=\"fa fa-minus-circle\" @click=\"minus(index)\" v-show=\"item.quantity!=0\"></i>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t\t<p><span v-show=\"item.quantity!=0\">{{item.quantity}}</span></p>\r\n\t\t\t\t\t<div>\r\n\t\t\t\t\t\t<i class=\"fa fa-plus-circle\" @click=\"add(index)\"></i>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</li>\r\n\t\t</ul>\r\n\r\n\t\t<!-- 购物车部分 -->\r\n\t\t<div class=\"cart\">\r\n\t\t\t<div class=\"cart-left\"><!-- 分成左右两块 -->\r\n\t\t\t\t<div class=\"cart-left-icon\" :style=\"totalQuantity==0?'background-color:#505051;':'background-color:#3190E8;'\">\r\n\t\t\t\t\t<i class=\"fa fa-shopping-cart\"></i>\r\n\t\t\t\t\t<div class=\"cart-left-icon-quantity\" v-show=\"totalQuantity!=0\">{{totalQuantity}}</div>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"cart-left-info\">\r\n\t\t\t\t\t<p>&#165;{{totalPrice}}</p>\r\n\t\t\t\t\t<p>另需配送费{{business.deliveryPrice}}元</p>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\r\n\t\t\t<div class=\"cart-right\">\r\n\t\t\t\t<!-- 不够起送费 -->\r\n\t\t\t\t<div class=\"cart-right-item\" v-show=\"totalSettle<business.starPrice\" style=\"background-color: #535356;cursor: default;\">\r\n\t\t\t\t            &#165;{{business.starPrice}}起送\r\n\t\t\t\t</div>\r\n\t\t\t\t\r\n\t\t\t\t<div class=\"cart-right-item\" @click=\"toOrder\" v-show=\"totalSettle>=business.starPrice\">\r\n\t\t\t\t\t去结算\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\timport {\r\n\t\tref,\r\n\t\tonMounted,\r\n\t\twatch,\n\t\tcomputed\r\n\t} from 'vue';\r\n\timport {\r\n\t\tuseRoute,\r\n\t\tuseRouter\r\n\t} from 'vue-router';\r\n\timport axios from 'axios';\r\n\timport qs from 'qs';\r\n\r\n\texport default {\r\n\t\tname: 'BusinessInfo',\r\n\t\tsetup() {\r\n\t\t\tconst route = useRoute();\r\n\t\t\tconst router = useRouter();\r\n\t\t\tconst businessId = ref(null);\r\n\t\t\t//const foodId = ref(null);\r\n\t\t\tconst business = ref({});\r\n\t\t\tconst foodArr = ref([]);\r\n\t\t\tconst user = ref(null);\r\n\t\t\tconst cartArr = ref([]);\r\n\t\t\tconst deliveryPrice = ref(0);\r\n\t\t\t\r\n\t\t\tconst fetchBusinessInfo = () => {\r\n\t\t\t\taxios\r\n\t\t\t\t\t.post('BusinessController/getBusinessById', qs.stringify({\r\n\t\t\t\t\t\tbusinessId: businessId.value\r\n\t\t\t\t\t}))\r\n\t\t\t\t\t.then(response => {\r\n\t\t\t\t\t\tbusiness.value = response.data;\r\n\t\t\t\t\t})\r\n\t\t\t\t\t.catch(error => {\r\n\t\t\t\t\t\tconsole.error(error);\r\n\t\t\t\t\t});\r\n\t\t\t};\r\n\r\n\t\t\tconst fetchFoodInfo = () => {\r\n\t\t\t\taxios\r\n\t\t\t\t\t.post('FoodController/listFoodByBusinessId', qs.stringify({\r\n\t\t\t\t\t\tbusinessId: businessId.value\r\n\t\t\t\t\t}))\r\n\t\t\t\t\t.then(response => {\r\n\t\t\t\t\t\tfoodArr.value = response.data;\r\n\t\t\t\t\t\tfor (let i = 0; i < foodArr.length; i++) {\r\n\t\t\t\t\t\t\tfoodArr.value[i].quantity = 0;\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\t//如果已登录，那么需要去查询购物车中是否已经选购了某个食品\r\n\t\t\t\t\t\tif (user != null) {\r\n\t\t\t\t\t\t\tlistCart();\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t})\r\n\t\t\t\t\t.catch(error => {\r\n\t\t\t\t\t\tconsole.error(error);\r\n\t\t\t\t\t});\r\n\t\t\t};\r\n\r\n\t\t\tconst getSessionStorage = () => {\r\n\t\t\t\tconst storedUser = sessionStorage.getItem('user');\r\n\t\t\t\tuser.value = storedUser ? JSON.parse(storedUser) : null;\r\n\t\t\t};\r\n\r\n\t\t\tconst add = (index) => {\r\n\t\t\t\tif (user.value === null) {\r\n\t\t\t\t\trouter.push({\r\n\t\t\t\t\t\tpath: '/login'\r\n\t\t\t\t\t});\r\n\t\t\t\t\treturn;\r\n\t\t\t\t}\r\n\t\t\t\tif (foodArr.value[index].quantity === 0) {\r\n\t\t\t\t\tsaveCart(index);\r\n\t\t\t\t} else {\r\n\t\t\t\t\tupdateCart(index, 1);\r\n\t\t\t\t}\r\n\t\t\t};\r\n\r\n\t\t\tconst minus = (index) => {\r\n\t\t\t\tif (user.value === null) {\r\n\t\t\t\t\trouter.push({\r\n\t\t\t\t\t\tpath: '/login'\r\n\t\t\t\t\t});\r\n\t\t\t\t\treturn;\r\n\t\t\t\t}\r\n\t\t\t\tif (foodArr.value[index].quantity > 1) {\r\n\t\t\t\t\tupdateCart(index, -1);\r\n\t\t\t\t} else {\r\n\t\t\t\t\tremoveCart(index);\r\n\t\t\t\t}\r\n\t\t\t};\r\n\r\n            const listCart = () => {\r\n            \taxios\r\n            \t\t.post('CartController/listCart', qs.stringify({\r\n            \t\t\tbusinessId: businessId.value,\r\n            \t\t\tuserId: user.value.userId,\r\n            \t\t\t//foodId: foodArr.value[index].foodId\r\n            \t\t}))\r\n            \t\t.then(response => {\r\n            \t\t\tlet cartArr = response.data;\r\n            \t\t\t//遍历所有食品列表，设置购物车中食品数量\r\n            \t\t\tfor (let foodItem of foodArr.value) {\r\n            \t\t\t\tfoodItem.quantity = 0;\r\n            \t\t\t\tfor (let cartItem of cartArr) {\r\n            \t\t\t\t\tif (cartItem.foodId == foodItem.foodId) {\r\n            \t\t\t\t\t\tfoodItem.quantity = cartItem.quantity;\r\n            \t\t\t\t\t}\r\n            \t\t\t\t}\r\n            \t\t\t}\r\n            \t\t\tfoodArr.value.sort();//确保vue监听到了它的变化\r\n            \t\t})\r\n            \t\t.catch(error => {\r\n            \t\t\tconsole.error(error);\r\n            \t\t});\r\n            }; //////////////////////////////////////////////////////////////////\r\n\t\t\t\r\n\t\t\tconst saveCart = (index) => {\r\n\t\t\t\taxios\r\n\t\t\t\t\t.post('CartController/saveCart', qs.stringify({\r\n\t\t\t\t\t\tbusinessId: businessId.value,\r\n\t\t\t\t\t\tuserId: user.value.userId,\r\n\t\t\t\t\t\tfoodId: foodArr.value[index].foodId\r\n\t\t\t\t\t}))\r\n\t\t\t\t\t.then(response => {\r\n\t\t\t\t\t\tif (response.data === 1) {\r\n\t\t\t\t\t\t\tfoodArr.value[index].quantity = 1;\r\n\t\t\t\t\t\t\tconst arr = [...foodArr.value]; // 将响应式变量转换为数组\r\n\t\t\t\t\t\t\tarr.sort(); // 对数组进行排序\r\n\t\t\t\t\t\t\tfoodArr.value = arr; // 将排序后的数组赋值回响应式变量\r\n\t\t\t\t\t\t} else {\r\n\t\t\t\t\t\t\talert('向购物车中添加食品失败');\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tconsole.log('saveadd');\r\n\t\t\t\t\t\tconsole.log(response.data);\r\n\t\t\t\t\t\tconsole.log('saveadd');\r\n\t\t\t\t\t})\r\n\t\t\t\t\t.catch(error => {\r\n\t\t\t\t\t\tconsole.error(error);\r\n\t\t\t\t\t})